server_tokens off;

server {
    listen 8080 default_server;
    # listen [::]:8080 default_server ipv6only=on;
    server_name %(hostname)s;

    root /var/lib/nginx/html;
    index index.html;

    location /admin {
        try_files $uri $uri/ =404;
    }
    # return 301 https://$server_name$request_uri;
}


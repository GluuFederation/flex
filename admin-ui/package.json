    "yup": "^1.0.2"
  },
  "resolutions": {
    "ansi-regex": "^5.0.1",
    "set-value": "^4.1.0",
    "jspdf": "^2.4.0",
    "autoprefixer": "^10.4.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:react/recommended",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
